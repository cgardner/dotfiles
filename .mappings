[general]
# GIT
"git/config"                    = "~/.gitconfig"
"git/template/hooks/commit-msg" = "~/.dotfiles/.git/hooks/commit-msg"

# TASKWARRIOR
"task/taskrc"                       = "~/.taskrc"
"task/taskhelper.yaml"              = "~/.config/taskhelper/taskhelper.yaml"
"task/hooks/on-exit"                = "~/.task/hooks/on-exit"
"task/hooks/on-modify.timewarrior"  = "~/.task/hooks/on-modify.timewarrior"
"task/hooks/on-add.default-project" = "~/.task/hooks/on-add.default-project"
"task/hooks/on-add.work-tags"       = "~/.task/hooks/on-add.work-tags"
"task/hooks/on-modify.work-tags"    = "~/.task/hooks/on-modify.work-tags"

# TIMEWARRIOR
"timewarrior/timewarrior.cfg" = "~/.timewarrior/timewarrior.cfg"

# VIM
"vim/nviminit.vim"      = "~/.config/nvim/init.vim"
"vim/vimrc.vim"         = "~/.vimrc"
"vim/coc-settings.json" = "~/.config/nvim/coc-settings.json"
"zsh/zshrc-manager.sh"  = "~/.zshrc"

# VIFM
"vifm/init"                       = "~/.config/vifm/vifmrc"
"vifm/colors/solarized-dark.vifm" = "~/.config/vifm/colors/solarized-dark.vifm"

# ALACRITTY
"term/alacritty.yml" = "~/.config/alacritty/alacritty.yml"

# TMUX
"tmux/tmux.conf"                = "~/.tmux.conf"
"tmux/templates/project.yml"    = "~/.tmuxinator/project.yml"
"tmux/templates/generic.yml"    = "~/.tmuxinator/generic.yml"
"tmux/templates/go-project.yml" = "~/.tmuxinator/go-project.yml"
"tmux/templates/dotfiles.yml"   = "~/.tmuxinator/dotfiles.yml"
"tmux/templates/comms.yml"      = "~/.tmuxinator/comms.yml"
"tmux/templates/flex.yml"       = "~/.tmuxinator/flex.yml"
"tmux/templates/dashboard.yml"  = "~/.tmuxinator/dashboard.yml"
"tmux/templates/tasks.yml"      = "~/.tmuxinator/tasks.yml"

# Mutt
"mutt/muttrc"  = "~/.muttrc"
"mutt/mailcap" = "~/.mailcap"
